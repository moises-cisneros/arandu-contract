{
  "name": "arandu-contract",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "generate": "yarn workspace hardhat generate",
    "account": "yarn workspace hardhat account",
    "deploy": "yarn workspace hardhat deploy",
    "deploy:arandu": "yarn workspace hardhat deploy:arandu",
    "deploy:arandu-fuji": "yarn workspace hardhat deploy:arandu-fuji",
    "wallets": "yarn workspace hardhat wallets",
    "register:wallets": "yarn workspace hardhat node scripts/registerWallets.js --network fuji",
    "faucet": "yarn workspace hardhat faucet",
    "compile": "yarn workspace hardhat compile",
    "test": "yarn workspace hardhat test",
    "deploy:fuji": "yarn workspace hardhat deploy:fuji",
    "deploy:sepolia": "yarn workspace hardhat deploy:sepolia",
    "deploy:avalanche": "yarn workspace hardhat deploy:avalanche",
    "deploy:local": "yarn workspace hardhat deploy:local",
    "start": "yarn workspace react-app dev",
    "build": "yarn workspace react-app build",
    "clean": "yarn workspace hardhat clean && rm -rf packages/hardhat/deployments && rm -rf packages/react-app/contracts",
    "chain": "yarn workspace hardhat chain"
  },
  "dependencies": {
    "ethers": "^6.15.0"
  }
}